{
  "name": "bluzphp/skeleton",
  "description": "Skeleton application for Bluz, a lightweight PHP framework",
  "minimum-stability": "dev",
  "type": "library",
  "require": {
    "php": ">=7.0",
    "bluzphp/framework": "~7.4",
    "hybridauth/hybridauth": "~2.8",
    "phpmailer/phpmailer": "~5.2",
    "zircote/swagger-php": "~2.0",
    "fortawesome/font-awesome": "~4.6",
    "twbs/bootstrap": "~3.3",
    "robmorgan/phinx": "~0.6",
    "public/swagger-ui": "dev-master",
    "js/jquery": "dev-master",
    "js/dropzone": "dev-master",
    "js/require": "dev-master"
  },
  "require-dev": {
    "bluzphp/bluzman": "dev-master",
    "satooshi/php-coveralls": "~1.0",
    "squizlabs/php_codesniffer": "~2.3",
    "zendframework/zend-dom": "~2.3"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "public/swagger-ui",
        "version": "dev-master",
        "license": "Apache-2.0",
        "dist":{
          "url":"https://github.com/swagger-api/swagger-ui/archive/v2.2.8.zip",
          "type":"zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "js/require",
        "version": "dev-master",
        "license": "MIT",
        "dist": {
          "url": "https://cdnjs.cloudflare.com/ajax/libs/require.js/2.3.3/require.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "js/jquery",
        "version": "dev-master",
        "license": "MIT",
        "dist": {
          "url": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "js/dropzone",
        "version": "dev-master",
        "license": "MIT",
        "dist": {
          "url": "https://cdnjs.cloudflare.com/ajax/libs/dropzone/4.3.0/min/dropzone-amd-module.min.js",
          "type": "file"
        }
      }
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "": "application/library",
      "Application\\": "application/models"
    },
    "files": [
      "application/_functions.php",
      "application/_loader.php"
    ]
  },
  "scripts": {
    "post-install-cmd": "php bin/install.php",
    "post-update-cmd": "php bin/update.php",
    "serve": "php -S 0.0.0.0:8080 -t ./public ./public/routing.php"
  },
  "authors": [
    {
      "name": "Bluz Skeleton Contributors",
      "homepage": "https://github.com/bluzphp/skeleton/graphs/contributors"
    }
  ],
  "support": {
    "issues": "https://github.com/bluzphp/skeleton/issues",
    "wiki": "https://github.com/bluzphp/skeleton/wiki"
  },
  "license": "MIT"
}
