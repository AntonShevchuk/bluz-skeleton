{
  "name": "bluzphp/skeleton",
  "description": "Skeleton application for Bluz, a lightweight PHP framework",
  "minimum-stability": "dev",
  "type": "library",
  "require": {
    "php": ">=7.0",
    "bluzphp/bluzman": "~2.2",
    "bluzphp/framework": "~7.7",
    "bluzphp/composer-plugin": "~2.1",
    "phpmailer/phpmailer": "~5.2",
    "zircote/swagger-php": "~2.0",
    "fortawesome/font-awesome": "~4.7",
    "twbs/bootstrap": "~4.0",
    "robmorgan/phinx": "~0.9",
    "public/swagger-ui": "dev-master",
    "public/jquery": "dev-master",
    "public/popper": "dev-master",
    "public/require": "dev-master"
  },
  "require-dev": {
    "codeception/codeception": "~2.3",
    "satooshi/php-coveralls": "~1.0",
    "squizlabs/php_codesniffer": "~3.0",
    "zendframework/zend-dom": "~2.6"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "public/swagger-ui",
        "version": "dev-master",
        "license": "Apache-2.0",
        "dist":{
          "url":"https://github.com/swagger-api/swagger-ui/archive/v3.2.1.zip",
          "type":"zip"
        },
        "extra": {
          "any":"value"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "public/require",
        "version": "dev-master",
        "license": "MIT",
        "dist": {
          "url": "https://cdnjs.cloudflare.com/ajax/libs/require.js/2.3.5/require.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "public/jquery",
        "version": "dev-master",
        "license": "MIT",
        "dist": {
          "url": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "public/popper",
        "version": "dev-master",
        "license": "MIT",
        "dist": {
          "url": "https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.5/umd/popper.min.js",
          "type": "file"
        }
      }
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "": "application/library",
      "Application\\": "application/models"
    },
    "files": [
      "application/_functions.php",
      "application/_loader.php"
    ]
  },
  "scripts": {
    "post-create-project-cmd": "php bin/install.php",
    "serve": "php -S 0.0.0.0:8080 -t ./public ./public/routing.php"
  },
  "extra": {
    "copy-files": {
      "public/swagger-ui/dist/": "public/api-reference/",
      "public/jquery/jquery.min.js": "public/js/vendor/jquery.min.js",
      "public/popper/popper.min.js": "public/js/vendor/popper.min.js",
      "public/require/require.min.js": "public/js/vendor/require.min.js",
      "twbs/bootstrap/dist/js/": "public/js/vendor/",
      "twbs/bootstrap/dist/css/": "public/css/",
      "twbs/bootstrap/dist/fonts/": "public/fonts/",
      "fortawesome/font-awesome/css/": "public/css/",
      "fortawesome/font-awesome/fonts/": "public/fonts/"
    }
  },
  "authors": [
    {
      "name": "Bluz Skeleton Contributors",
      "homepage": "https://github.com/bluzphp/skeleton/graphs/contributors"
    }
  ],
  "support": {
    "issues": "https://github.com/bluzphp/skeleton/issues",
    "wiki": "https://github.com/bluzphp/skeleton/wiki"
  },
  "license": "MIT"
}
